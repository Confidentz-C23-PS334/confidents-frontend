<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="scanResultVM" type="com.confidents.app.modules.scanresult.data.viewmodel.ScanResultVM"/>
    </data>
    <LinearLayout android:id="@+id/linearColumngroup10007"
        android:layout_height="match_parent"
        android:layout_marginBottom="@dimen/_419pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_51">
        <FrameLayout android:gravity="start"
            android:id="@+id/frameStackgroup10007"
            android:layout_height="@dimen/_321pxv" android:layout_width="match_parent">
            <ImageView android:id="@+id/imageGroup10007"
                android:layout_gravity="start"
                android:layout_height="@dimen/_321pxh"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_group10007"
                tools:ignore="ContentDescription" tools:src="@drawable/img_group10007"/>
            <FrameLayout android:id="@+id/frameStack48532"
                android:layout_gravity="top|start"
                android:layout_height="@dimen/_229pxv"
                android:layout_marginBottom="@dimen/_10pxv" android:layout_width="match_parent">
                <ImageView android:id="@+id/image48532"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_229pxh"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_48532}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_48532"/>
                <LinearLayout android:id="@+id/linearColumnleftside"
                    android:layout_gravity="top|center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_12pxv"
                    android:layout_marginEnd="@dimen/_14pxh"
                    android:layout_marginStart="@dimen/_14pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <androidx.appcompat.widget.Toolbar
                        android:gravity="start"
                        android:id="@+id/toolbarToolbar"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" app:contentInsetStart="0dp">
                        <LinearLayout
                            android:id="@+id/linearIPhoneXorne"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent" android:orientation="horizontal">
                            <ImageView android:id="@+id/imageLeftSide"
                                android:layout_height="@dimen/_21pxh"
                                android:layout_width="@dimen/_54pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_leftside_black_901"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_leftside_black_901"/>
                            <ImageView android:id="@+id/imageSignal"
                                android:layout_height="@dimen/_11pxh"
                                android:layout_marginBottom="@dimen/_4pxv"
                                android:layout_marginStart="@dimen/_271pxh"
                                android:layout_marginTop="@dimen/_5pxv"
                                android:layout_width="@dimen/_66pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_signal_black_902"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_signal_black_902"/>
                        </LinearLayout>
                    </androidx.appcompat.widget.Toolbar>
                    <ImageView android:id="@+id/imageArrowleft"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_12pxh"
                        android:layout_marginEnd="@dimen/_15pxh"
                        android:layout_marginStart="@dimen/_15pxh"
                        android:layout_marginTop="@dimen/_42pxv"
                        android:layout_width="@dimen/_12pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowleft"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                </LinearLayout>
            </FrameLayout>
        </FrameLayout>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearScanResult"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_32pxv"
            android:layout_width="wrap_content"
            android:orientation="vertical" style="@style/groupStylegray_51">
            <TextView android:ellipsize="end"
                android:gravity="center_horizontal"
                android:id="@+id/txtGreenApple"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_1pxh"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_6pxh"
                android:singleLine="true"
                android:text="@{scanResultVM.scanResultModel.txtGreenApple}"
                style="@style/txtInterregular28" tools:text="@string/msg_agus_s_scan_res"/>
            <TextView android:ellipsize="end" android:id="@+id/txtDate"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_19pxv"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_5pxh"
                android:singleLine="true"
                android:text="@{scanResultVM.scanResultModel.txtDate}"
                style="@style/txtInterregular20" tools:text="@string/lbl_28_may_2023"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtDescription"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_width="wrap_content"
                android:lineSpacingExtra="@dimen/_5pxh"
                android:singleLine="true"
                android:text="@{scanResultVM.scanResultModel.txtDescription}"
                style="@style/txtInterregular20_1" tools:text="@string/lbl_description"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtGreenapplesha"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_4pxh"
                android:singleLine="true"
                android:text="@{scanResultVM.scanResultModel.txtGreenapplesha}"
                style="@style/txtInterregular16_5" tools:text="@string/msg_gigi_ini_terind"/>
        </LinearLayout>
    </LinearLayout>
</layout>
