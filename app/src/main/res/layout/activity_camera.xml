<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="cameraVM" type="com.confidents.app.modules.camera.data.viewmodel.CameraVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumn48531"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_51">
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumnleftside"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_14pxh"
            android:layout_marginStart="@dimen/_21pxh"
            android:layout_marginTop="@dimen/_12pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:id="@+id/linearIPhoneXorne"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageLeftSide"
                        android:layout_height="@dimen/_21pxh"
                        android:layout_width="@dimen/_54pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_leftside"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_leftside"/>
                    <ImageView android:id="@+id/imageSignal"
                        android:layout_height="@dimen/_11pxh"
                        android:layout_marginBottom="@dimen/_4pxv"
                        android:layout_marginStart="@dimen/_271pxh"
                        android:layout_marginTop="@dimen/_5pxv"
                        android:layout_width="@dimen/_66pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_signal"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_signal"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <ImageView android:id="@+id/imageArrowleft"
                android:layout_height="@dimen/_12pxh"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_33pxv"
                android:layout_width="@dimen/_11pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_arrowleft_white_a700"
                tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft_white_a700"/>
        </LinearLayout>
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true" android:gravity="start"
            android:id="@+id/scrollViewScrollview"
            android:layout_height="0dp" android:layout_weight="1" android:layout_width="match_parent">
            <FrameLayout android:id="@+id/frameCamera"
                android:layout_height="match_parent" android:layout_width="match_parent">
                <LinearLayout
                    android:id="@+id/linearColumnrectangle1320"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylebluegray_103">
                    <View
                        android:background="@drawable/rectangle_gradient_s_cyan_300_7f_e_black_900_00"
                        android:id="@+id/viewRectangle1320"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_252pxv"
                        android:layout_marginBottom="@dimen/_674pxv" android:layout_width="@dimen/_428pxh"/>
                </LinearLayout>
                <ImageView android:id="@+id/image48531"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_704pxv"
                    android:layout_marginBottom="@dimen/_93pxv"
                    android:layout_marginTop="@dimen/_93pxv"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_48531}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_48531"/>
                <ImageView android:id="@+id/imageFrame9976"
                    android:layout_gravity="bottom|end"
                    android:layout_height="@dimen/_64pxh"
                    android:layout_marginBottom="@dimen/_20pxv"
                    android:layout_marginEnd="@dimen/_77pxh"
                    android:layout_marginStart="@dimen/_77pxh"
                    android:layout_marginTop="@dimen/_20pxv"
                    android:layout_width="@dimen/_169pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_frame9976"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_frame9976"/>
            </FrameLayout>
        </ScrollView>
    </LinearLayout>
</layout>
